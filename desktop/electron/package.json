{
  "name": "post-app",
  "version": "1.0.0",
  "description": "An Electron application with Solid and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "transpile": "bunx jco transpile wasm/repo.wasm -o dist/transpiled --instantiation=async"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@atproto/crypto": "^0.4.5",
    "@bytecodealliance/jco": "^1.15.4",
    "@bytecodealliance/preview2-shim": "^0.17.5",
    "@ipld/dag-cbor": "^9.2.5",
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-fs": "~2",
    "@tauri-apps/plugin-opener": "^2",
    "@types/varint": "^6.0.3",
    "multiformats": "^13.4.1",
    "solid-icons": "^1.1.0",
    "tailwindcss": "^4.1.17",
    "varint": "^6.0.0",
    "web-did-resolver": "^2.0.31"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@types/node": "^22.18.6",
    "electron": "^38.1.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.1",
    "eslint": "^9.36.0",
    "eslint-plugin-solid": "^0.14.5",
    "prettier": "^3.6.2",
    "solid-js": "^1.9.9",
    "typescript": "^5.9.2",
    "vite": "^7.1.6",
    "vite-plugin-solid": "^2.11.8"
  }
}
