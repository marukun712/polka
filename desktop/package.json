{
	"name": "desktop",
	"version": "1.0.0",
	"main": "dist/index.js",
	"type": "module",
	"private": true,
	"scripts": {
		"build:ui": "cd ui && bun run build",
		"build:main": "tsc",
		"build": "bun run build:ui && bun run build:main",
		"start": "electron .",
		"package": "bun run build && electron-builder",
		"format": "biome format ./",
		"format:fix": "biome format --write ./",
		"lint": "biome lint ./",
		"lint:fix": "biome lint --write ./",
		"check": "biome check ./",
		"check:fix": "biome check --write ./"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.8",
		"@types/bun": "latest",
		"@types/node": "^20",
		"electron": "^39.2.6",
		"electron-builder": "^25.1.8",
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@atcute/tid": "^1.0.3",
		"@atproto/crypto": "^0.4.5",
		"@bytecodealliance/preview2-shim": "^0.17.5",
		"@ipld/dag-cbor": "^9.2.5",
		"@noble/curves": "^2.0.1",
		"@noble/hashes": "^2.0.1",
		"@types/varint": "^6.0.3",
		"did-resolver": "^4.1.0",
		"multiformats": "^13.4.1",
		"solid-icons": "^1.1.0",
		"varint": "^6.0.0",
		"web-did-resolver": "^2.0.32"
	}
}
